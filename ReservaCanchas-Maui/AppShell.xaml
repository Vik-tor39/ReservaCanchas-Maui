<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ReservaCanchas_Maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:ReservaCanchas_Maui.Views"
    Shell.FlyoutBehavior="Flyout"
    Title="ReservaCanchas_Maui">

    <FlyoutItem 
        Title="Inicio"
        Icon="dotnet_bot.png">
        <ShellContent
            Title="Inicio"
            ContentTemplate="{DataTemplate views:ComplejosPage}"
            Route="ComplejosPage" />
    </FlyoutItem>

    <FlyoutItem 
        Title="Trabajo con nosotros"
        Icon="dotnet_bot.png">
        <ShellContent
            Title="Trabajo con nosotros"/>
    </FlyoutItem>

    <FlyoutItem 
        Title="Cerrar Sesión"
        Icon="dotnet_bot.png">
        <ShellContent
            Title="Cerrar Sesión"/>
    </FlyoutItem>

</Shell>
